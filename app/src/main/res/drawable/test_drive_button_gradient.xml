<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:angle="90"
                android:startColor="#5D4037"
                android:endColor="#3E2723"
                android:type="linear" />
            <corners android:radius="12dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:angle="90"
                android:startColor="#795548"
                android:endColor="#5D4037"
                android:type="linear" />
            <corners android:radius="12dp" />
        </shape>
    </item>
</selector>